<% if (@discussions_user.role.name == 'proband') %>
  $(".probands_list").children().remove()
  $(".probands_list").html('' +
  '<%= escape_javascript(render partial: "discussions_users/list_users",
              locals: { discussions_users: @probands, id: "probands", page_name: "probands_page" }) %>')
<% end %>

<% if (@discussions_user.role.name == 'observer') %>
  $(".observers_list").children().remove()
  $(".observers_list").html('' +
  '<%= escape_javascript(render partial: "discussions_users/list_users",
            locals: { discussions_users: @observers, id: "observers", page_name: "observers_page" }) %>')
<% end %>