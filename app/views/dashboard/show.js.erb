<% if current_user.is_admin? %>
	$('.discussions_list').children().remove()
	$('.discussions_list').html('' +
	    '<%= escape_javascript(render partial: 'discussions/list_discussions', locals: { discussions: @discussions }) %>')

<% else %>
	$('.discussions_list').children().remove()
	$('.discussions_list').html('' +
	    '<%= escape_javascript(render partial: 'discussions_users/list_discussions', locals: { discussions_users: @discussions_users }) %>')
<% end %>